div.golden-dict-padding {
	width: 100%;
	height: 1px;
}

div.word-entry.non-golden-dict-padding, div.key-word-entry.non-golden-dict-padding {
	margin-bottom: 8px;
}

p.non-golden-dict-padding {
	margin-top: 5px;
	margin-bottom: 5px;
}

h2 {
	font-size: 18px;
}

span.word-delimiter {
	display: none;
}

div.head-words a {
	margin-right: 10px;
	display: inline-block;
	font-weight: bold;
    font-size: 1.1em;
}

div.head-words a.key-head-word, div.words-entries.default-show-key-head-word-only div.key-word-entry.key-head-word, .key-sense {
	color: #da3e3e;
	background-color: #e9f1df;
}

span.item-num {
	display: none;
}

h2.sigil_not_in_toc[onclick], div.sense>p {
	cursor: pointer;
}

p.sense {
	margin-top: 10px;
    margin-bottom: 10px;
    font-size: 1.1em;
    color: #0c85cc;
    font-weight: bold;
}

span.textItalic {
    color: darkslategray;	
}

.flashing-shown-word-entry {
	color: yellow;
	background-color: red;	
}

div.word-entry, div.key-word-entry {
	margin-bottom: 5px;
}

div.word-entry>p:first-child {
	display: inline;
	margin-right: -5px;
}

p.meaning {
	color: mediumvioletred;
	display: inline-block;
	margin-right: 5px;
}

p.derive {
	color: green;
	display: inline-block;
}

p.use {
	margin-left: 10px;
	color: #b9651e;
}

p.example {
	display: block;
}

p.example-en {
	display: inline-block;
	margin-left: 10px;
	color: gray;	
}

p.example-cn {
	display: inline-block;
	margin-left: 10px;
